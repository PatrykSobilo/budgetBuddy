{
    "name": "patryk-sobio/budget_buddy",
    "description": "Aplikacja PHP - PrzyszlyProgramista",
    "type": "project",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "gemini-api-php/client": "^1.7",
        "symfony/http-client": "^7.3",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "src/Framework",
            "App\\": "src/App"
        },
        "files": [
            "src/App/Config/Routes.php",
            "src/App/Config/Middleware.php"
        ]
    },
    "scripts": {
        "budget_buddy": "php cli.php"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
